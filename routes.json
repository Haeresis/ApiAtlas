[{
	"url": "/comments/(:id/)?",
	"get": true,
	"view": "default.htm",
	"variation": "comments.json",
	"controller": "comments.js",
    "mimeType": "application/json",
    "headers": { "Link": "</api/comments/>; rel=collection" }
}, {
	"url": "/comments/",
	"post": true,
